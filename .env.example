# Notion Integration
NOTION_API_KEY=your-notion-key
NOTION_BRANDING_PAGE_ID=your-page-id

# n8n Webhook (for triggering workflows)
N8N_WEBHOOK_URL=http://localhost:5678/webhook-test/run-icos
